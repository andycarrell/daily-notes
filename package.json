{
  "name": "daily-notes",
  "version": "0.0.1",
  "author": "andycarrell",
  "description": "",
  "license": "ISC",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:andycarrell/daily-notes.git"
  },
  "scripts": {
    "build:js": "esbuild src/index.tsx --bundle --minify --outfile=dist/bundle.js",
    "build:css": "postcss src/styles/index.scss -o dist/index.css",
    "build": "npm run prime && npm run build:js && npm run build:css",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:check": "eslint -c eslint.config.js .",
    "prime": "copyfiles public/* dist --up=1",
    "start": "npm run prime && concurrently \"npm run types:check -- --watch\" \"npm run build:js -- --servedir=dist\" \"npm run build:css -- --watch\"",
    "types:check": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "1.0.6",
    "@tailwindcss/typography": "0.5.4",
    "@tanstack/react-query": "4.0.10",
    "@tiptap/extension-blockquote": "2.0.0-beta.29",
    "@tiptap/extension-bold": "2.0.0-beta.28",
    "@tiptap/extension-bullet-list": "2.0.0-beta.29",
    "@tiptap/extension-code": "2.0.0-beta.28",
    "@tiptap/extension-code-block": "2.0.0-beta.42",
    "@tiptap/extension-document": "2.0.0-beta.17",
    "@tiptap/extension-hard-break": "2.0.0-beta.33",
    "@tiptap/extension-heading": "2.0.0-beta.29",
    "@tiptap/extension-history": "2.0.0-beta.26",
    "@tiptap/extension-horizontal-rule": "2.0.0-beta.36",
    "@tiptap/extension-italic": "2.0.0-beta.28",
    "@tiptap/extension-link": "2.0.0-beta.43",
    "@tiptap/extension-list-item": "2.0.0-beta.23",
    "@tiptap/extension-ordered-list": "2.0.0-beta.30",
    "@tiptap/extension-paragraph": "2.0.0-beta.26",
    "@tiptap/extension-strike": "2.0.0-beta.29",
    "@tiptap/extension-task-item": "2.0.0-beta.37",
    "@tiptap/extension-task-list": "2.0.0-beta.29",
    "@tiptap/extension-text": "2.0.0-beta.17",
    "@tiptap/extension-typography": "2.0.0-beta.22",
    "@tiptap/extension-underline": "2.0.0-beta.25",
    "@tiptap/react": "2.0.0-beta.114",
    "autoprefixer": "10.4.8",
    "esbuild": "0.14.51",
    "localforage": "1.10.0",
    "nanoid": "4.0.0",
    "postcss": "8.4.16",
    "postcss-cli": "10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.1.8"
  },
  "devDependencies": {
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.32.0",
    "concurrently": "7.3.0",
    "copyfiles": "2.4.1",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss-import": "14.1.0",
    "postcss-nested": "5.0.6",
    "postcss-scss": "4.0.4",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
